import java.util.Scanner;

class math {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int Sc=in.nextInt();
        int i=0;

        System.out.println(fib(Sc));
    }
    public static long fib(int n)
    {
        if(n==1||n==2)
        {
            return 1;
        }
        else
            return fib(n-1)+fib(n-2);

    }
}
